{
  "key": "test-generator-app",
  "name": "TestGen AI",
  "description": "G√©n√©rateur de tests automatique pour vos user stories",
  "vendor": {
    "name": "QalilabAi",
    "url": "https://jirasecondprime.onrender.com"
  },
  "baseUrl": "https://jirasecondprime.onrender.com",
  "authentication": {
    "type": "jwt"
  },
  "apiVersion": 1,
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": [
    "read",
    "write"
  ],
  "modules": {
    "jiraIssueGlances": [
      {
        "key": "generate-tests-glance",
        "name": {
          "value": "üß† G√©n√©rer les tests"
        },
        "icon": {
          "width": 24,
          "height": 24,
          "url": "/static/brain.svg"
        },
        "content": {
          "type": "label",
          "label": {
            "value": "G√©n√©rer des tests"
          }
        },
        "target": {
          "type": "web_panel",
          "url": "/test-generator"
        },
        "conditions": [
          {
            "condition": "issue_type",
            "params": {
              "issueTypes": ["Story", "Task", "Bug"]
            }
          }
        ]
      }
    ],
    "confluenceContentBylineItems": [
      {
        "key": "generate-tests-confluence",
        "name": {
          "value": "üß† G√©n√©rer les tests"
        },
        "icon": {
          "width": 24,
          "height": 24,
          "url": "/static/brain.svg"
        },
        "url": "/test-generator-confluence",
        "target": {
          "type": "dialog",
          "options": {
            "size": "medium",
            "chrome": true
          }
        }
      }
    ],
    "dialogs": [
      {
        "key": "test-generator-dialog",
        "options": {
          "size": "medium",
          "chrome": true
        },
        "url": "/test-generator"
      }
    ]
  }
}